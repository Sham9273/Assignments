import java.util.*;
class RemoveArrayDuplicate {
    public static void main(String[] args) {
        int[]a={1,2,5,4,3};
        Arrays.sort(a);
        System.out.println(Arrays.toString(a));
        for(int i=a.length-1;i>=0;i--){
            System.out.print(a[i]+" " );
        }
        
    }
}
